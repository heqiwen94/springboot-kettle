    <sql>select &#xd;&#xa;v.patient_id&#x7c;&#x7c;&#x27;&#x2b;&#x27;&#x7c;&#x7c;v.visit_id IN_SNO, &#xd;&#xa;&#x27;42920056-6&#x27; ORG_CODE, &#xd;&#xa;dia.diagnosis_date DIAGNOSIS_DATE, &#xd;&#xa;v.patient_id&#x7c;&#x7c;&#x27;&#x2b;&#x27;&#x7c;&#x7c;v.visit_id&#x7c;&#x7c;&#x27;&#x2b;&#x27;&#x7c;&#x7c;dia.diagnosis_type&#x7c;&#x7c;&#x27;&#x2b;&#x27;&#x7c;&#x7c;dia.diagnosis_no DIAGNOSIS_SNO, &#xd;&#xa;case dia.diagnosis_type when &#x27;1&#x27; then 4 when &#x27;2&#x27; then 1 when &#x27;3&#x27; then 3 &#xd;&#xa;else 4 end DIAGNOSIS_TYPE, &#xd;&#xa;case when dia.diagnosis_type&#x3d;&#x27;3&#x27; and dia.diagnosis_no&#x3d;1 then &#x27;1&#x27; else &#x27;0&#x27; end  DISEASE_MAIN_FLAG, &#xd;&#xa;case when dia.diagnosis_type&#x3d;&#x27;A&#x27; or dia.diagnosis_type&#x3d;&#x27;B&#x27; or dia.diagnosis_type&#x3d;&#x27;C&#x27;  then  &#x27;0&#x27; else &#x27;1&#x27; end WESTORCH, &#xd;&#xa;dia.diagnosis_code DIAGNOSIS_CODE, &#xd;&#xa;&#x27;2&#x27; ICD_SYSTEM, &#xd;&#xa;dia.diagnosis_desc DIAGNOSIS_NAME, &#xd;&#xa;dia.treat_result DIAGNOSIS_STATE, &#xd;&#xa;&#x27;&#x27; REMARK&#xd;&#xa;from PAT_VISIT v &#xd;&#xa;inner join DIAGNOSIS dia on dia.patient_id&#x3d;v.patient_id and dia.visit_id&#x3d;v.visit_id&#xd;&#xa;where dia.diagnosis_date&#x3e;&#x3d;to_date&#x28;&#x24;&#x7b;startTime&#x7d;,&#x27;yyyy-mm-dd hh24&#x3a;mi&#x3a;ss&#x27;&#x29;  and  dia.diagnosis_date&#x3c;&#x3d;to_date&#x28;&#x24;&#x7b;endTime&#x7d;,&#x27;yyyy-mm-dd hh24&#x3a;mi&#x3a;ss&#x27;&#x29; </sql>
